{
  "env": {
    "development": {
      "plugins": [[
        "react-transform", {
          "transforms": [{
            "transform": "react-transform-hmr",
            "imports": ["react"],
            "locals": ["module"]
          }, {
            "transform": "react-transform-catch-errors",
            "imports": ["react", "redbox-react"]
          }]
        }]
      ]
    }
  },
  "plugins": ["react-require", "transform-runtime"],
  "presets": ["es2015", "stage-0", "react"]
}
